/* Write your PL/SQL query statement below */
# REDという言葉が含まれている取引先を持っていないセールスマンたちの名前を求め。

SELECT name AS 'name'
FROM SalesPerson
WHERE sales_id NOT IN (
    SELECT sales_id
    FROM Orders
    WHERE com_id IN (
        SELECT com_id
        FROM Company
        WHERE 'RED' = name
    )
)
